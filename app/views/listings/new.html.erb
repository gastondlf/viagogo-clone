<h1>Create a new listing</h1>
<div class="container">
  <%= simple_form_for ([@event, @listing]) do |f| %>
  <%= f.input :description, label: "Description" %>
  <%= f.input :quantity, label: "Quantity" %>
  <%= f.association :ticket_category%>
  <%= f.submit "Create Tickets", class: "btn btn-primary" %>
<% end %>
</div>
</div>
